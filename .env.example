# ================================
# YouTube API Configuration
# ================================
# YouTube Data API v3 密钥
YOUTUBE_API_KEY=your_youtube_api_key_here

# 调试模式
DEBUG_MODE=false

# ================================
# Proxy Configuration (for YouTube API)
# ================================
# HTTP/HTTPS代理配置（用于YouTube API请求）
PROXY_HOST=your_proxy_host
PROXY_PORT=your_proxy_port
PROXY_USERNAME=your_proxy_username
PROXY_PASSWORD=your_proxy_password

# ================================
# SOCKS5 Proxy Configuration
# ================================
# SOCKS5代理配置（用于youtube-transcript-api）
SOCKS5_PROXY_HOST=127.0.0.1
SOCKS5_PROXY_PORT=7897

# ================================
# NewAPI Configuration (Gemini)
# ================================
# NewAPI服务的API密钥
NEWAPI_API_KEY=your_newapi_key_here

# NewAPI服务的基础URL
NEWAPI_BASE_URL=http://localhost:51099

# ================================
# TTS Service Configuration
# ================================
# 旧版TTS服务URL（EasyVoice）
TTS_OLD_URL=http://localhost:3000

# 新版TTS服务URL（公开API）
TTS_NEW_URL=http://localhost:3031

# ================================
# Draft Service Configuration
# ================================
# 剪映草稿本地目录
DRAFT_LOCAL_DIR=test

# ================================
# Optional: Additional Services
# ================================
# 如需添加其他服务配置，请在此处添加
# CUSTOM_SERVICE_URL=http://localhost:8080
# CUSTOM_API_KEY=your_api_key_here